uav_parser_type: "dji_parser/DjiParser"
uav_controller_timer_duration: 20

uav_arm_system_handler_config {
  arm_controller_timer_duration: 20
  arm_parser_type: "SimpleArm"
}

base_config {
  state_machine_timer_duration: 20
  status_timer_duration: 50
}

uav_system_config {
  minimum_battery_percent: 25
  minimum_takeoff_height: 0.8
  landing_height: 0.1
  position_controller_config {
    goal_position_tolerance {
      x: 0.05
      y: 0.05
      z: 0.05
    }
  }

  velocity_based_position_controller_config {
    position_gain: 0.7
    yaw_gain: 0.3
    max_velocity: 0.25
    max_yaw_rate: 0.3
    yaw_i_gain: 0.01
    position_i_gain: 0.04
    integrator_saturation_gain: 1.35
    position_controller_config {
      goal_position_tolerance {
        x: 0.04
        y: 0.04
        z: 0.04
      }
      goal_yaw_tolerance: 0.1
    }
  }
  velocity_controller_config {
    goal_velocity_tolerance {
      vx: 0.1
      vy: 0.1
      vz: 0.1
    }
  }

  uav_vision_system_config {
    tracker_type: "Alvar"

    camera_transform: 0.2093
    camera_transform: 0.0063
    camera_transform: -0.0836
    camera_transform: 1.9372
    camera_transform: 0
    camera_transform: 1.5794

    tracking_offset_transform: 0.0
    tracking_offset_transform: 0.0
    tracking_offset_transform: 0.0
    tracking_offset_transform: -1.5708
    tracking_offset_transform: 0.0
    tracking_offset_transform: 0.0

    desired_visual_servoing_distance: 0.48

    constant_heading_depth_controller_config {
      radial_gain: 0.6
      tangential_gain: 1.3
      yaw_gain: 0.3
      max_velocity: 0.2
      min_velocity: 0.0
      max_yaw_rate: 0.3
      position_controller_config {
        goal_position_tolerance {
          x: 0.05
          y: 0.05
          z: 0.05
        }
        goal_yaw_tolerance: 0.08
      }
    }

    relative_pose_goals {
      position {
        x: 0
        y: -.63
        z: 0.26 #0.27
      }
      yaw: 1.5708
    }

    relative_pose_goals {
      position {
        x: 0
        y: -.53
        z: 0.26 #0.27
      }
      yaw: 1.5708
    }

    relative_pose_goals {
      position {
        x: 0
        y: -.61
        z: -0.02
      }
      yaw: 1.5708
    }

    velocity_based_relative_pose_controller_config {
      velocity_based_position_controller_config {
        position_gain: 0.7
        yaw_gain: 0.3
        max_velocity: 0.25
        max_yaw_rate: 0.3
        yaw_i_gain: 0.01
        position_i_gain: 0.04
        integrator_saturation_gain: 1.35
        position_controller_config {
          goal_position_tolerance {
            x: 0.04
            y: 0.04
            z: 0.04
          }
          goal_yaw_tolerance: 0.1
        }
      }
    }

    uav_arm_system_config {
      grip_timeout: 20000

      arm_goal_transform: 0.0
      arm_goal_transform: -.1 #-0.093
      arm_goal_transform: 0.1
      arm_goal_transform: 0.0
      arm_goal_transform: 0.0
      arm_goal_transform: 0.0

      arm_goal_transform: 0.0
      arm_goal_transform: -.1 #-0.093
      arm_goal_transform: -0.01
      arm_goal_transform: 0.0
      arm_goal_transform: 0.0
      arm_goal_transform: 0.0

      arm_transform: 0.17
      arm_transform: 0.0
      arm_transform: -0.109
      arm_transform: 0.0
      arm_transform: -1.5707
      arm_transform: 3.141

      position_controller_config {
        goal_position_tolerance {
          x: 0.02
          y: 0.02
          z: 0.02
        }
        goal_rotation_tolerance: 6
      }
      way_points {
        position {
          x: 2.0
